{
  "name": "web-page-analyzer-front-end",
  "version": "0.0.1",
  "scripts": {
    "start": "ng serve",
    "start:local": "ng serve --prod",
    "build:prod": "ng build --base-href=/ --prod",
    "test": "jest --watchAll",
    "test:prod": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --sort --format json --marker _t",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
