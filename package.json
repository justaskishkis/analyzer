    "ts-node": "~7.0.1",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3",
    "wallaby-webpack": "^3.9.15",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./src/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@companyName-lib/(.*)": "<rootDir>/src/companyName-lib/$1",
      "@companyName-ui-components": "<rootDir>/src/companyName-ui-components"
    }
  }
}
